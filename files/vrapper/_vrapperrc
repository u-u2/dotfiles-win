# vim
set autoindent

set hlsearch
set incsearch

set number

set tabstop=4
set shiftwidth=0
set expandtab


set clipboard=unnamed

# vrapper
set contentassistmode
set startnormalmode
set imdisable

nmap <C-[><C-[> :noh<Enter>
nmap <S-h> :tabprevious<Enter>
nmap <S-l> :tabnext<Enter>
nmap <Space>w :w<Enter>
nmap <Space>q :q<Enter>
nmap <Space>r :reg<Enter>
nmap <Space>m :marks<Enter>
nmap <Space>b :buffers<Enter>
nmap <Space>e :files<Enter>
